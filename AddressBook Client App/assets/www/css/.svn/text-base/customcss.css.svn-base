/*Custom Overlay On image*/
.imagebefore{
	position:relative;
    width:50%;
    height:auto;
    overflow: hidden;
}
.imagebefore img {
    width:100%;
    vertical-align:top;
}
.imagebefore:after, .imagebefore:before {
    position:absolute;
    opacity:0;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
}
.imagebefore:after {
    content:'\A';
    width:100%; height:100%;
    top:0; left:0;
    background:rgba(0,0,0,0);
}
.imagebefore:before {
    width:100%;
    color:#fff;
    z-index:1;
    bottom:0;
    padding:4px 10px;
    text-align:center;
    background:rgba(0,0,0,0);
    box-sizing:border-box;
    -moz-box-sizing:border-box;
}
.imagebefore:after, .imagebefore:before {
    opacity:1;
}

.image {
    position:relative;
    width:50%;
    height:auto;
}
.image img {
    width:100%;
    vertical-align:top;
}
.image:after, .image:before {
    position:absolute;
    opacity:0;
    transition: all 0.5s;
    -webkit-transition: all 0.5s;
}
.image:after {
    content:'\A';
    width:100%; height:100%;
    top:0; left:0;
    background:rgba(0,0,0,0.6);
}
.image:before {
    content: attr(data-content);
    width:100%;
    color:#fff;
    z-index:1;
    bottom:0;
    padding:4px 10px;
    text-align:center;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
}
.image:after, .image:before {
    opacity:1;
}

