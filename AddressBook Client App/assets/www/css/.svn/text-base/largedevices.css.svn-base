body *{
	font-size:30px !important;
}
input::-webkit-input-placeholder {
   font-size:30px !important;
}

input:-moz-placeholder {
    font-size:30px !important;
}

/*Custom Details Table*/
#detailsPageTable img{
	height: 60px;
}
/*Custom Header*/
.ui-header *{
	font-size: 60px !important;
	color: #000 !important;
}
.ui-li-heading{
	padding-bottom: 20px;
}
.ui-li-desc{
	position: relative;
}

#nameHeader {
	font-size: 60px !important;
}

.ui-header .ui-title{
	font-size: 50px !important;
	line-height: 80px;
}

.ui-header .right-btn img{
	height:80px;
}
.ui-header .left-btn img{
	height:80px;
}
/*Custom Header Finished*/

/*Custom Icons Panel*/
.ui-icon-customsync, .ui-icon-customimportfile, .ui-icon-customadd, .ui-icon-customclearlocaldata, .ui-icon-customviewall, .ui-icon-customedit, .ui-icon-customdelete, .ui-icon-customviewbycatagory{
	background-size: 40px 40px;
	width: 45px;
	height: 45px;
}
/*Custom Icon Panel Finished*/

.smallfonts{
	padding-left: 10px;
}

/*Custom Popup CallMsg*/
#callMsgDialog img{
	height:70px;
}

/*Custom Overlay on Image*/
.imagebefore{
	position:relative;
    width:50%;
    height:auto;
}

.image{
	position:relative;
    width:50%;
    height:auto;
}

/*Custom Ui Panel*/
.ui-panel {
	width:40% !important;
}

.ui-panel .ui-link-inherit{
	padding: 15px !important;
	font-size: 25px !important;
}

.ui-panel .ui-link-inherit {
	font-size: 30px !important;
	padding: 15px !important;
}

/*Custom Index Page Listview*/
.contactListCountryFlag{
	height: 30px;
	padding-left: 20px;
}
#contactList .ui-li-thumb{
	max-height: 100px;
	max-width: 100px;
}

.ui-block-a,.ui-block-b{
	height: 100px;
}
.phoneIconIndexPage{
	height: 40px;
	padding-left: 10px;
}
/*Custom Index Page Listview Finished*/

/*Custom Details Page Flag*/
#detailsPageFlag{
	height: 2em;
}

/*Custom Choose Catagory*/
#chooseCatagory{
	height: 100%
}
#chooseCatagory * {
	font-size: 30px;
}
/*Custom Choose Catagory Finished*/

